#include <iostream>
#include <ctime>
using namespace std;
void main()
{
	setlocale(LC_ALL, "ru");
	const int ROW = 3;
	const int COL = 4;
	int arr[ROW][COL];
	arr[0][0] = 5;
	arr[0][1] = 8;
	arr[0][2] = 13;
	arr[0][3] = 53;
	arr[1][0] = 27;
	arr[1][1] = 22;
	arr[1][2] = 67;
	arr[1][3] = 3;
	arr[2][0] = 32;
	arr[2][1] = 9;
	arr[2][2] = 1;
	arr[2][3] = 78;
	cout << arr[0][0] << endl;
	cout << arr[0][1] << endl;
	cout << arr[0][2] << endl;
	cout << arr[0][3] << endl;
	cout << arr[1][0] << endl;
	cout << arr[1][1] << endl;
	cout << arr[1][2] << endl;
	cout << arr[1][3] << endl;
	cout << arr[2][0] << endl;
	cout << arr[2][1] << endl;
	cout << arr[2][2] << endl;
	cout << arr[2][3] << endl;
}
